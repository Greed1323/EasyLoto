<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane alignment="CENTER" hgap="5.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.william.easyloto.ihm.controller.MenuController">
   <children>
      <Button alignment="CENTER" mnemonicParsing="false" onAction="#goShowCarton" text="Afficher les cartons" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
